    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime-corejs3": "^7.4.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-brands-svg-icons": "^5.8.2",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "babel-loader": "^8.0.6",
    "core-js": "^3.1.3",
    "css-loader": "^2.1.1",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scrollbars-custom": "^4.0.12",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.2.4",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "scripts": {
    "build": "rimraf ./dist && webpack -p --mode production --optimize-minimize --colors --progress --config webpack.config.js",
    "build:dev": "rimraf ./dist && webpack -d --mode development --colors --progress --config webpack.config.js",
    "devserver": "webpack-dev-server --open -d --mode development --hot --colors --progress --devtool source-map --config webpack.config.js "
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 120
  }
}
